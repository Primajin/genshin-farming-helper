{
  "name": "genshin-farming-helper",
  "homepage": "https://Primajin.github.io/genshin-farming-helper",
  "version": "1.0.0",
  "description": "Proof of concept of a helper for keeping track while farming in Genshin Impact",
  "keywords": [
    "genshin impact",
    "genshin",
    "impact",
    "farming"
  ],
  "main": "src/index.jsx",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@fontsource/material-icons": "^4.5.4",
    "genshin-db": "^4.3.2",
    "gh-pages": "^4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@babel/runtime": "7.20.1",
    "@testing-library/react": "^13.4.0",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "xo": "^0.52.4"
  },
  "scripts": {
    "build": "react-scripts build",
    "coverage": "npm test -- --coverage",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "lint:watch": "npx nodemon -x npm run lint:fix --ext js,jsx,json",
    "predeploy": "npm run build",
    "pretest": "npm run lint",
    "publish": "npm run deploy",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "xo": {
    "extends": [
      "xo",
      "xo-react"
    ],
    "overrides": [
      {
        "files": "src/**/__tests__/*",
        "plugins": [
          "jest"
        ],
        "extends": [
          "plugin:jest/recommended",
          "plugin:jest/style"
        ]
      }
    ],
    "rules": {
      "import/extensions": "off",
      "react/react-in-jsx-scope": "off",
      "react/no-unknown-property": [2,{"ignore": ["css"]}]
    }
  }
}
