{
  "name": "genshin-farming-helper",
  "homepage": "https://Primajin.github.io/genshin-farming-helper",
  "version": "1.0.0",
  "description": "Proof of concept of a helper for keeping track while farming in Genshin Impact",
  "author": "Jannis Hell",
  "private": true,
  "keywords": [
    "genshin impact",
    "genshin",
    "impact",
    "farming"
  ],
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@fontsource/material-icons": "^4.5.4",
    "gatsby": "^5.2.0",
    "gatsby-plugin-emotion": "^8.2.0",
    "gatsby-plugin-image": "^3.2.0",
    "gatsby-plugin-manifest": "^5.2.0",
    "gatsby-plugin-sharp": "^5.2.0",
    "gatsby-source-filesystem": "^5.2.0",
    "gatsby-transformer-sharp": "^5.2.0",
    "genshin-db": "^4.3.2",
    "gh-pages": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "xo": "^0.53.1"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "coverage": "npm test -- --coverage",
    "deploy": "gh-pages -d build",
    "develop": "gatsby develop",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "lint:watch": "npx nodemon -x npm run lint:fix --ext js,jsx,json",
    "predeploy": "npm run build",
    "pretest": "npm run lint",
    "serve": "gatsby serve",
    "start": "gatsby develop"
  },
  "xo": {
    "extends": [
      "xo",
      "xo-react"
    ],
    "rules": {
      "import/extensions": "off",
      "react/react-in-jsx-scope": "off",
      "react/no-unknown-property": [2,{"ignore": ["css"]}]
    }
  }
}
